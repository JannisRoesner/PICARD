{
  "name": "sitzungsmaster-web",
  "version": "1.0.0",
  "description": "Web-basierte Anwendung für digitale Moderationskarten bei Karnevalssitzungen",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.0",
    "puppeteer": "^22.8.2",
    "archiver": "^6.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "karneval",
    "moderation",
    "sitzung",
    "web",
    "real-time"
  ],
  "author": "Jannis Rösner",
  "license": "MIT"
} 